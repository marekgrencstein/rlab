<h1>Lab Registration</h1>
<%=  form_for @lab %>
<div class="field">
    <%= lab=Lab.all   
       select_tag "Lab", options_from_collection_for_select(lab, 'id', 'name', selected = nil), :multiple => false %>
</div>

<div class="field">
	<%= label_tag :password%><br>
	<%= password_field_tag :password%>
</div>

<div class="actions">
		<%= submit_tag "register"%>
</div>

	<br/>
<%=link_to "Home", root_url%>
<p>Find me in app/views/home/labreg.html.erb</p>


