<p id="notice"><%= notice %></p>

<h1>Listing Bookings</h1>

<table>
  <thead>
    <tr>
      <th>Start</th>
      <th>Step</th>
      <th>Lab</th>
      <th>User</th>
      <th>Group</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.start %></td>
        <td><%= booking.step %></td>
        <td><%= booking.lab.name %></td>
        <td><%= booking.user %></td>
        <td><%= booking.group %></td>

        <td><%= link_to 'Show', booking %></td>
        <td><%= link_to 'Edit', edit_booking_path(booking) %></td>
        <td><%= link_to 'Destroy', booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>

    <% end %>
    <div class="field">
  <%= label_tag :labs %>

</div>
    
  </tbody>
</table>

<br>

<%= link_to 'New Booking', new_booking_path %>
