<!DOCTYPE html>
<html>
<head>
  <title>Remotelab</title>
  <!--<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>-->
  <link rel="stylesheet" type="text/css" href="/assets/theme.css" />
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="header" >
		<div id="title-panel" >REMOTE LAB</div>
		<div id="login-panel" >
			User: <input type="text" class="text-field" />
			Password:</td><td><input type="password" class="text-field" />
			<input type="button" id="login-panel-button" value="Login" />
		</div>
	</div>
	<div id="two-collums" >
		<div id="navigator" >
			<div class="nav-option" > Person </div>
			<div class="nav-option" > Groups </div>
			<div class="nav-option" > Labs </div>
			<div class="nav-option" > Booking </div>
		</div>
		<div id="content">

			<%= yield %>
			<!--<%=request.raw_post%>-->
			
		</div>
		
	</div>
	
	<script src="/assets/styleHandler.js"></script>

</body>
</html>
